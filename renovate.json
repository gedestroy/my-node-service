{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^Jenkinsfile$"],
      "matchStrings": [
        "[']*(?<depName>[a-z0-9-']+)[']*: \\[version: '(?<currentValue>[a-z0-9.-]+)'\\]"
      ],
      "datasourceTemplate": "docker",
      "versioningTemplate": "docker"
    }
  ]
}
